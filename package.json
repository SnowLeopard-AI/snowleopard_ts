{
  "name": "@snowleopard/client",
  "type": "module",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript client library for Snow Leopard Playground APIs - works in both Node.js and browsers",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts && prettier --check \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "snowleopard",
    "ai",
    "api-client",
    "typescript",
    "browser",
    "nodejs"
  ],
  "author": "Snowy <hello@snowleopard.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SnowLeopard-AI/snowleopard_ts.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.0.0"
  },
  "browser": {
    "stream": false
  }
}
